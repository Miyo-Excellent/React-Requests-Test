  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "NODE_ENV=production BABEL_ENV=production && babel-node node_modules/.bin/webpack --progress --display-modules --config webpack/webpack.config.client.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production && babel-node node_modules/.bin/webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf dist/ & rimraf public/app & rimraf public/css",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "remotedev": "remotedev --hostname=localhost --port=8001",
    "local-server-external": "http-server babel-node ../src/server/index.js -p 6060 -a 127.0.0.2",
    "start": "npm run clean && NODE_ENV=development BABEL_ENV=development NODE_PORT=3000 && babel-node ./src/server/index.js",
    "start-analyzer": "npm run clean && NODE_ENV=development BABEL_ENV=development ANALYZER=true && babel-node src/server",
    "start-production": "npm run build && NODE_ENV=production BABEL_ENV=production NODE_PORT=3000 && babel-node src/server",
    "start-deployment": "npm run build && NODE_PORT=3000 NODE_ENV=production BABEL_ENV=production DEPLOYMENT=true && babel-node src/server/index.js",
    "test": "echo \"Error: no test specified\"",
    "watch-client": "webpack --watch --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --config webpack/webpack.config.server.js"
  },
